<Window x:Class="LEDController.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LEDController"
        xmlns:main="clr-namespace:LEDController.Main.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="5">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" />
        </Grid.ColumnDefinitions>

        <StackPanel>
            <Button x:Name="test" Content="TestFN" Height="20" VerticalAlignment="Top" Click="test_Click" />
            <Button x:Name="getList" Content="GetList" Height="20" VerticalAlignment="Top" Click="getList_Click"  />
            <ListBox x:Name="comList"/>
        </StackPanel>
    </Grid>
</Window>
